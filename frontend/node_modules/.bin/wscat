#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/ws/bin/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/ws/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules:/Users/eliezeribghi/Desktop/ofour/node_modules:/Users/eliezeribghi/Desktop/node_modules:/Users/eliezeribghi/node_modules:/Users/node_modules:/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/ws/bin/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/ws/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules:/Users/eliezeribghi/Desktop/ofour/node_modules:/Users/eliezeribghi/Desktop/node_modules:/Users/eliezeribghi/node_modules:/Users/node_modules:/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ws/bin/wscat" "$@"
else
  exec node  "$basedir/../ws/bin/wscat" "$@"
fi
