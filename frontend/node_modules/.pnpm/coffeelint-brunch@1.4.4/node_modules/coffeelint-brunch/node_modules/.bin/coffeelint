#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules/coffeelint/bin/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules/coffeelint/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules/coffeelint/bin/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules/coffeelint/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/coffeelint@2.1.0/node_modules:/Users/eliezeribghi/Desktop/ofour/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../coffeelint@2.1.0/node_modules/coffeelint/bin/coffeelint" "$@"
else
  exec node  "$basedir/../../../../../coffeelint@2.1.0/node_modules/coffeelint/bin/coffeelint" "$@"
fi
